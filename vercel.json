{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "images/*",
            "use": "@vercel/static"
        },
        {
            "src": "style.css",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 
