{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 
